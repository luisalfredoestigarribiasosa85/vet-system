{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vet-system-backend",
  "version": "1.0.0",
  "description": "Backend para el sistema de veterinaria",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --setupFilesAfterEnv=./tests/setup.js",
    "test:watch": "jest --watch --setupFilesAfterEnv=./tests/setup.js",
    "test:coverage": "jest --coverage --setupFilesAfterEnv=./tests/setup.js",
    "seed": "node scripts/seed.js",
    "backfill:appointments": "node scripts/backfillAppointments.js",
    "migrate-saas": "node scripts/migrate-saas.js",
    "create-plans": "node scripts/create-default-plans.js",
    "setup-saas": "npm run migrate-saas && npm run create-plans"
  },
  "keywords": [
    "veterinaria",
    "backend",
    "api"
  ],
  "author": "Luis Estigarribia",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "sequelize": "^6.37.7",
    "stripe": "^17.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vet-system-backend": "file:",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  }
}
